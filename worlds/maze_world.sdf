            </box>
          </geometry>
          <material>
            <ambient>0.3 0.3 0.8 1</ambient>
            <diffuse>0.5 0.5 1 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

    <!-- Horizontal wall 3 -->
    <model name="wall_h3">
      <static>true</static>
      <pose>1.5 3 0.25 0 0 0</pose>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>1 0.1 0.5</size>
            </box>
          </geometry>
        </collision>
        <visual name="visual">
